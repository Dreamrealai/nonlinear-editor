# Test Environment Variables
# These are used during Jest test execution
# DO NOT commit real credentials to this file

NODE_ENV=test

# Supabase Configuration (Mock URLs for testing)
NEXT_PUBLIC_SUPABASE_URL=https://test.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=test-anon-key-mock-value-do-not-use-in-production
SUPABASE_SERVICE_ROLE_KEY=test-service-role-key-mock-value-do-not-use-in-production

# Database Configuration (Not used in unit tests, but needed for config validation)
DATABASE_URL=postgresql://test:test@localhost:5432/test_db

# Google Cloud Configuration (Mock values)
GOOGLE_CLOUD_PROJECT_ID=test-project-id
GOOGLE_CLOUD_LOCATION=us-central1
GOOGLE_APPLICATION_CREDENTIALS=./test-credentials.json

# Google AI Configuration (Mock keys)
NEXT_PUBLIC_GEMINI_API_KEY=test-gemini-key-mock
VERTEX_AI_PROJECT_ID=test-vertex-project

# Stripe Configuration (Test mode keys)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_mock_key
STRIPE_SECRET_KEY=sk_test_mock_key
STRIPE_WEBHOOK_SECRET=whsec_test_mock_secret

# Price IDs (Test mode)
STRIPE_PRICE_ID_PRO_MONTHLY=price_test_pro_monthly
STRIPE_PRICE_ID_PRO_YEARLY=price_test_pro_yearly
STRIPE_PRICE_ID_ENTERPRISE_MONTHLY=price_test_enterprise_monthly
STRIPE_PRICE_ID_ENTERPRISE_YEARLY=price_test_enterprise_yearly

# Application URLs
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_API_URL=http://localhost:3000/api

# Feature Flags
NEXT_PUBLIC_ENABLE_AI_FEATURES=true
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_ERROR_TRACKING=false

# Storage Configuration
STORAGE_BUCKET_NAME=test-bucket
STORAGE_MAX_FILE_SIZE=104857600

# Rate Limiting (Relaxed for testing)
RATE_LIMIT_ENABLED=false
RATE_LIMIT_MAX_REQUESTS=1000
RATE_LIMIT_WINDOW_MS=60000

# Session Configuration
SESSION_SECRET=test-session-secret-min-32-chars-long-mock-value

# Email Configuration (Mock SMTP)
SMTP_HOST=localhost
SMTP_PORT=1025
SMTP_USER=test
SMTP_PASSWORD=test
SMTP_FROM=test@example.com

# Monitoring and Observability (Disabled in tests)
SENTRY_DSN=
SENTRY_ENVIRONMENT=test
SENTRY_ENABLED=false

# PostHog Analytics (Disabled in tests)
NEXT_PUBLIC_POSTHOG_KEY=
NEXT_PUBLIC_POSTHOG_HOST=
POSTHOG_ENABLED=false

# Redis (Mock for caching tests)
REDIS_URL=redis://localhost:6379
UPSTASH_REDIS_REST_URL=https://test.upstash.io
UPSTASH_REDIS_REST_TOKEN=test-token

# Misc
LOG_LEVEL=error
DEBUG=false
ENABLE_SOURCE_MAPS=false
